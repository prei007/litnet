@prefix : <http://purl.org/mp/> .
@prefix dct: <http://dublincore.org/documents/2012/06/14/dcmi-terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/mp/> .

<http://purl.org/mp/> rdf:type owl:Ontology ;
                       rdfs:comment """Micropublications: a Semantic Model for Claims, Evidence, Arguments and Annotations in Biomedical Communications.

Author:         	Tim Clark (1,2,3)
Contributors:   	Paolo Ciccarese (1,2), Carole Goble (3)

Version 1.17, May 12, 2016.

(1) Massachusetts General Hospital, Dept. of Neurology
(2) Harvard Medical School
(3) University of Manchester, School of Computer Science 

""" ;
                       owl:priorVersion "1.17" ;
                       owl:versionInfo "1.18" .

#################################################################
#    Object Properties
#################################################################

###  http://purl.org/mp/arguedBy
:arguedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :supports ;
          owl:inverseOf :argues ;
          rdfs:domain :Claim ;
          rdfs:range :Micropublication .


###  http://purl.org/mp/argues
:argues rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :supportedBy ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :Micropublication ;
        rdfs:range :Claim .


###  http://purl.org/mp/assertedBy
:assertedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :elementOf ;
            owl:inverseOf :asserts ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Representation ;
            rdfs:range :Micropublication ;
            owl:propertyDisjointWith :quotedBy ;
            rdfs:comment "A Representation assertedBy a Micropublication is originally instantiated by that Micropublication." .


###  http://purl.org/mp/asserts
:asserts rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasElement ;
         rdf:type owl:InverseFunctionalProperty ;
         rdfs:domain :Micropublication ;
         rdfs:range :Representation ;
         owl:propertyDisjointWith :quotes .


###  http://purl.org/mp/attributedTo
:attributedTo rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ,
                                 prov:wasAttributedTo ;
              rdfs:domain :Artifact ;
              rdfs:range :Agent .


###  http://purl.org/mp/attributionAsAuthor
:attributionAsAuthor rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf prov:hasAttribution ;
                     rdfs:domain :Artifact ;
                     rdfs:range :Agent .


###  http://purl.org/mp/attributionAsCurator
:attributionAsCurator rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf prov:hasAttribution ;
                      rdfs:domain :Artifact ;
                      rdfs:range :Agent .


###  http://purl.org/mp/attributionAsEditor
:attributionAsEditor rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf prov:hasAttribution ;
                     rdfs:domain :Artifact ;
                     rdfs:range :Agent .


###  http://purl.org/mp/attributionAsPublisher
:attributionAsPublisher rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf prov:hasAttribution ;
                        rdfs:domain :Artifact ;
                        rdfs:range :Agent .


###  http://purl.org/mp/attributionOfAgent
:attributionOfAgent rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf prov:agent ;
                    rdfs:domain :Attribution ;
                    rdfs:range :Agent .


###  http://purl.org/mp/authoredBy
:authoredBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :attributedTo ;
            rdfs:domain :Artifact ;
            rdfs:range :Agent ;
            owl:propertyChainAxiom ( :attributionAsAuthor
                                     :attributionOfAgent
                                   ) .


###  http://purl.org/mp/challengeGraphElementOf
:challengeGraphElementOf rdf:type owl:ObjectProperty ;
                         owl:inverseOf :hasChallengeGraphElement ;
                         rdfs:domain :Micropublication ;
                         rdfs:range :Representation .


###  http://purl.org/mp/challengedBy
:challengedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :challenges ;
              rdfs:domain :Representation ;
              rdfs:range :Representation .


###  http://purl.org/mp/challenges
:challenges rdf:type owl:ObjectProperty ;
            rdfs:domain :Representation ;
            rdfs:range :Representation ;
            rdfs:comment "The challenges property is inferred when a Representation either directlyChallenges another, or indirectlyChallenges it by undercutting (directlyChallenges) a Representation which supports it." .


###  http://purl.org/mp/curatedBy
:curatedBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :attributedTo ;
           rdfs:domain :Artifact ;
           rdfs:range :Agent ;
           owl:propertyChainAxiom ( :attributionAsCurator
                                    :attributionOfAgent
                                  ) .


###  http://purl.org/mp/dataSupports
:dataSupports rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :evidenceSupports ;
              owl:inverseOf :supportedByData ;
              rdfs:domain :Data ;
              rdfs:range :Representation .


###  http://purl.org/mp/directlyChallengedBy
:directlyChallengedBy rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :challengedBy ;
                      owl:inverseOf :directlyChallenges ;
                      rdfs:domain :Representation ;
                      rdfs:range :Representation .


###  http://purl.org/mp/directlyChallenges
:directlyChallenges rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :challenges ;
                    rdfs:domain :Representation ;
                    rdfs:range :Representation .


###  http://purl.org/mp/editedBy
:editedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :attributedTo ;
          rdfs:domain :Artifact ;
          rdfs:range :Agent ;
          owl:propertyChainAxiom ( :attributionAsEditor
                                   :attributionOfAgent
                                 ) .


###  http://purl.org/mp/elementOf
:elementOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Representation ;
           rdfs:range :Micropublication ;
           rdfs:comment "A Representation is defined as an elementOf a Micropublication if that Micropublication either asserts or quotes it." .


###  http://purl.org/mp/evidenceSupports
:evidenceSupports rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :supports ;
                  owl:inverseOf :supportedByEvidence ;
                  rdfs:domain :Representation ;
                  rdfs:range :Representation .


###  http://purl.org/mp/hasAttribution
:hasAttribution rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :supportedBy ,
                                   prov:qualifiedAttribution ;
                rdfs:domain :Artifact ;
                rdfs:range :Attribution .


###  http://purl.org/mp/hasChallengeGraphElement
:hasChallengeGraphElement rdf:type owl:ObjectProperty ;
                          rdfs:domain :Micropublication ;
                          rdfs:range :Representation ;
                          rdfs:seeAlso "hasSupportGraphElement" .


###  http://purl.org/mp/hasElement
:hasElement rdf:type owl:ObjectProperty ;
            rdfs:domain :Micropublication ;
            rdfs:range :Representation .


###  http://purl.org/mp/hasHolotype
:hasHolotype rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :representedBy ;
             owl:inverseOf :holotypeFor ;
             rdfs:domain :Statement ;
             rdfs:range :Statement .


###  http://purl.org/mp/hasSupportGraphElement
:hasSupportGraphElement rdf:type owl:ObjectProperty ;
                        owl:inverseOf :supportGraphElementOf ;
                        rdfs:domain :Micropublication ;
                        rdfs:range :Representation ;
                        rdfs:comment "Representations related to the Claim of a Micropublication by the supports property, and which are elementsOf that Micropublication, constitute its Support Graph, being related to the Micropublication by the property hasSupportGraphElement." .


###  http://purl.org/mp/holotypeFor
:holotypeFor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :represents ;
             rdfs:domain :Holotype ;
             rdfs:range :Statement .


###  http://purl.org/mp/indirectlyChallengedBy
:indirectlyChallengedBy rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :challengedBy ;
                        owl:inverseOf :indirectlyChallenges ;
                        rdfs:domain :Representation ;
                        rdfs:range :Representation .


###  http://purl.org/mp/indirectlyChallenges
:indirectlyChallenges rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :challenges ;
                      rdfs:domain :Representation ;
                      rdfs:range :Representation ;
                      owl:propertyChainAxiom ( :directlyChallenges
                                               :supports
                                             ) .


###  http://purl.org/mp/methodSupports
:methodSupports rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :evidenceSupports ;
                owl:inverseOf :supportedByMethod ;
                rdfs:domain :Method ;
                rdfs:range :Representation .


###  http://purl.org/mp/publishedBy
:publishedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :attributedTo ;
             rdfs:domain :Artifact ;
             rdfs:range :Agent ;
             owl:propertyChainAxiom ( :attributionAsPublisher
                                      :attributionOfAgent
                                    ) .


###  http://purl.org/mp/qualifiedBy
:qualifiedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :qualifies ;
             rdfs:domain prov:Entity ;
             rdfs:range :Sentence .


###  http://purl.org/mp/qualifies
:qualifies rdf:type owl:ObjectProperty ;
           rdfs:range prov:Entity .


###  http://purl.org/mp/quotedBy
:quotedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :elementOf ;
          owl:inverseOf :quotes ;
          rdfs:domain :Claim ;
          rdfs:range :Representation ;
          rdfs:comment "A Representation quotedBy a Micropublication is referred to by that Micropublication, after first being instantiated by another Micropublication." .


###  http://purl.org/mp/quotes
:quotes rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :hasElement ;
        rdfs:domain :Micropublication ;
        rdfs:range :Representation .


###  http://purl.org/mp/referencedBy
:referencedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :qualifiedBy ;
              owl:inverseOf :references ;
              rdfs:domain prov:Entity ;
              rdfs:range :Reference .


###  http://purl.org/mp/references
:references rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :qualifies ;
            rdfs:domain :Reference ;
            rdfs:range prov:Entity .


###  http://purl.org/mp/representedBy
:representedBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :represents ;
               rdfs:domain prov:Entity ;
               rdfs:range :Representation .


###  http://purl.org/mp/represents
:represents rdf:type owl:ObjectProperty ;
            rdfs:domain :Representation ;
            rdfs:range prov:Entity .


###  http://purl.org/mp/semtaggedBy
:semtaggedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :qualifiedBy ;
             owl:inverseOf :semtags ;
             rdfs:domain prov:Entity ;
             rdfs:range :SemanticQualifier .


###  http://purl.org/mp/semtags
:semtags rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :qualifies ;
         rdfs:domain :SemanticQualifier ;
         rdfs:range prov:Entity .


###  http://purl.org/mp/supportGraphElementOf
:supportGraphElementOf rdf:type owl:ObjectProperty ;
                       rdfs:domain :Representation ;
                       rdfs:range :Micropublication .


###  http://purl.org/mp/supportedBy
:supportedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :supports ;
             rdf:type owl:TransitiveProperty ;
             rdfs:domain :Representation ;
             rdfs:range :Representation .


###  http://purl.org/mp/supportedByData
:supportedByData rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :supportedByEvidence .


###  http://purl.org/mp/supportedByEvidence
:supportedByEvidence rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :supportedBy ;
                     rdf:type owl:TransitiveProperty ;
                     rdfs:domain :Representation ;
                     rdfs:range :Representation ;
                     rdfs:comment "If Data supports a Statement, that Statement is supportedByData. Data may be supportedByMethod if a Method supports it." .


###  http://purl.org/mp/supportedByMethod
:supportedByMethod rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :supportedByEvidence .


###  http://purl.org/mp/supports
:supports rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:domain :Representation ;
          rdfs:range :Representation ;
          rdfs:comment """The supports property is a transitive relation between Representations.

The supports relationships amongst the Representations in a single Micropublications are structured as a directed acyclic graph (DAG), whose root is the Micropublication’s Claim.""" .


###  http://www.w3.org/ns/prov#agent
prov:agent rdf:type owl:ObjectProperty ;
           rdfs:domain :Attribution ;
           rdfs:range :Agent .


###  http://www.w3.org/ns/prov#hasAttribution
prov:hasAttribution rdf:type owl:ObjectProperty ;
                    rdfs:domain :Artifact ;
                    rdfs:range :Agent .


###  http://www.w3.org/ns/prov#qualifiedAttribution
prov:qualifiedAttribution rdf:type owl:ObjectProperty ;
                          rdfs:domain :Artifact ;
                          rdfs:range :Agent .


###  http://www.w3.org/ns/prov#wasAttributedTo
prov:wasAttributedTo rdf:type owl:ObjectProperty ;
                     rdfs:domain :Artifact ;
                     rdfs:range :Agent .


#################################################################
#    Data properties
#################################################################

###  http://dublincore.org/documents/2012/06/14/dcmi-terms/bibliographicCitation
dct:bibliographicCitation rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Reference ;
                          rdfs:range xsd:string .


###  http://purl.org/mp/atTime
:atTime rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain :Attribution ;
        rdfs:range xsd:dateTime ;
        rdfs:comment "YYYY-MM-DDThh:mm:ss" .


###  http://purl.org/mp/authoredOn
:authoredOn rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Artifact ;
            rdfs:range xsd:dateTime ;
            rdfs:comment "format: YYYY-MM-DDThh:mm:ss" .


###  http://purl.org/mp/citation
:citation rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf dct:bibliographicCitation ,
                             :value ;
          rdfs:domain :Reference ;
          rdfs:range xsd:string .


###  http://purl.org/mp/curatedOn
:curatedOn rdf:type owl:DatatypeProperty ;
           rdfs:domain :Artifact ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "YYYY-MM-DDThh:mm:ss" .


###  http://purl.org/mp/editedOn
:editedOn rdf:type owl:DatatypeProperty ;
          rdfs:domain :Artifact ;
          rdfs:range xsd:dateTime ;
          rdfs:comment "YYYY-MM-DDThh:mm:ss" .


###  http://purl.org/mp/publishedOn
:publishedOn rdf:type owl:DatatypeProperty ;
             rdfs:domain :Artifact ;
             rdfs:range xsd:dateTime ;
             rdfs:comment "YYYY-MM-DDThh:mm:ss" .


###  http://purl.org/mp/statement
:statement rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :value ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Sentence ;
           rdfs:range xsd:string .


###  http://purl.org/mp/value
:value rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :Artifact ;
       rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://purl.org/mp/Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf prov:Entity ;
       rdfs:comment "An Agent is an Entity that makes, modifies, consumes or uses an Artifact." ,
                    "changed from prov:Agent to mp:Agent PR" .


###  http://purl.org/mp/ArticleText
:ArticleText rdf:type owl:Class ;
             rdfs:subClassOf :Representation .


###  http://purl.org/mp/Artifact
:Artifact rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity ;
          rdfs:comment "An Artifact is an Entity produced, modified, consumed or used by an Agent" .


###  http://purl.org/mp/Attribution
:Attribution rdf:type owl:Class ;
             rdfs:subClassOf :Representation ;
             rdfs:comment "The minimal level of support for any Statement is its Attribution to some Agent." .


###  http://purl.org/mp/Claim
:Claim rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Statement
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :arguedBy ;
                                                    owl:someValuesFrom :Micropublication
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :Statement ;
       rdfs:comment "A Claim is the single principal statement arguedBy a Micropublication, with statements and/or evidence deployed to support it. Its support may also include contrary statements or evidence; and/or the claim may dispute claims made by other arguments. A claim's support is modelled as a graph." .


###  http://purl.org/mp/Data
:Data rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :Representation
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :dataSupports ;
                                                   owl:someValuesFrom :Representation
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :Representation ;
      rdfs:comment "If Data supports a Statement, that Statement is supportedByData. Data may be supportedByMethod if a Method supports it." .


###  http://purl.org/mp/Holotype
:Holotype rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Statement
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :holotypeFor ;
                                                       owl:someValuesFrom :Statement
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Statement .


###  http://purl.org/mp/Material
:Material rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Method
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :represents ;
                                                       owl:someValuesFrom :Artifact
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Method ;
          rdfs:comment "A Material supports any Method of which it is a component." .


###  http://purl.org/mp/Method
:Method rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Representation
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :methodSupports ;
                                                     owl:someValuesFrom :Representation
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Representation ;
        rdfs:comment "A description of how data were obtained. Yt specifies a Procedure, and may refer to some Material as a component of the recipe. A Material supports any Method of which it is a component." .


###  http://purl.org/mp/Micropublication
:Micropublication rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Representation
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :argues ;
                                                               owl:someValuesFrom :Statement
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Representation ;
                  rdfs:comment """A Micropublication is a set of Representations, having supports and/or challenges relations to one another and potentially to those which are an elementOf other Micropublications.

Micropublications represent scientific arguments. The goal of an argument is to induce belief. An argument (therefore a micropublication) argues a principal claim, with statements and/or evidence deployed to support it.""" .


###  http://purl.org/mp/Procedure
:Procedure rdf:type owl:Class ;
           rdfs:subClassOf :Method .


###  http://purl.org/mp/QualifiedStatement
:QualifiedStatement rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Statement
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :qualifiedBy ;
                                                                 owl:someValuesFrom :Sentence
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Statement .


###  http://purl.org/mp/Qualifier
:Qualifier rdf:type owl:Class ;
           rdfs:subClassOf :Sentence ;
           rdfs:comment "Sentences which qualify a statement." .


###  http://purl.org/mp/Reference
:Reference rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Sentence
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :citation ;
                                                        owl:someValuesFrom xsd:string
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Qualifier .


###  http://purl.org/mp/ReferencedStatement
:ReferencedStatement rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :QualifiedStatement
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :referencedBy ;
                                                                  owl:someValuesFrom :Reference
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :QualifiedStatement .


###  http://purl.org/mp/Representation
:Representation rdf:type owl:Class ;
                rdfs:subClassOf :Artifact ;
                rdfs:comment "A Representation is an Artifact which represents something. A Representation may be a Sentence, Data, Method, Micropublication, Attribution, or ArticleText." .


###  http://purl.org/mp/SemanticQualifier
:SemanticQualifier rdf:type owl:Class ;
                   rdfs:subClassOf :Qualifier .


###  http://purl.org/mp/Sentence
:Sentence rdf:type owl:Class ;
          rdfs:subClassOf :Representation ;
          rdfs:comment "A Sentence is a well-formed series of symbols intended to convey meaning." .


###  http://purl.org/mp/Statement
:Statement rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Sentence
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :statement ;
                                                        owl:someValuesFrom xsd:string
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Sentence ;
           rdfs:comment "A Statement is a declarative Sentence." .


###  http://purl.org/mp/TaggedStatement
:TaggedStatement rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :QualifiedStatement
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :semtaggedBy ;
                                                              owl:someValuesFrom :SemanticQualifier
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf :QualifiedStatement .


###  http://www.w3.org/ns/prov#Entity
prov:Entity rdf:type owl:Class ;
            rdfs:comment "Entities are things which may be discussed, real or imaginary." .


###  http://www.w3.org/ns/prov#Organization
prov:Organization rdf:type owl:Class ;
                  rdfs:subClassOf :Agent .


###  http://www.w3.org/ns/prov#Person
prov:Person rdf:type owl:Class ;
            rdfs:subClassOf :Agent .


#################################################################
#    Rules
#################################################################

<urn:swrl#a> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl#b> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl#c> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl#t> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl#r> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

<urn:swrl#m> rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
   <http://www.w3.org/2003/11/swrl#body> [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                           rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
                                                       <http://www.w3.org/2003/11/swrl#propertyPredicate> :attributionAsAuthor ;
                                                       <http://www.w3.org/2003/11/swrl#argument1> <urn:swrl#a> ;
                                                       <http://www.w3.org/2003/11/swrl#argument2> <urn:swrl#b>
                                                     ] ;
                                           rdf:rest [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
                                                      rdf:first [ rdf:type <http://www.w3.org/2003/11/swrl#IndividualPropertyAtom> ;
