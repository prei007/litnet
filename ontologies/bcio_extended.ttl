                                          obo:IAO_0000078 "External"^^<http://www.learn-web.com/ontologies/2023/bcio_extended/w3.org/2001/XMLSchema#string> ;
                                          obo:IAO_0000112 "An ongoing consideration regarding this class is whether a socially shared representation can be said to be purely cognitive."^^<http://www.learn-web.com/ontologies/2023/bcio_extended/w3.org/2001/XMLSchema#string> ;
