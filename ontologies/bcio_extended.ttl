                                                       rdfs:subClassOf <http://humanbehaviourchange.org/ontology/BCIO_006052> ;
                                                       obo:IAO_0000078 "Published"^^<http://www.learn-web.com/ontologies/2023/bcio_extended/w3.org/2001/XMLSchema#string> ;
                                                       obo:IAO_0000115 "Knowledge regarding a therapy or medication for a health problem."^^<http://www.learn-web.com/ontologies/2023/bcio_extended/w3.org/2001/XMLSchema#string> ;
